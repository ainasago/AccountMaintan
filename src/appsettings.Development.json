{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=accounts.db",
    "IdentityConnection": "Data Source=accounts.db",
    "HangfireConnection": "Data Source=accounts.db"
  },
  "Security": {
    "Development": {
      "ContentSecurityPolicy": {
        "DefaultSrc": "'self' 'unsafe-inline' 'unsafe-eval' data: https:",
        "ScriptSrc": "'self' 'unsafe-inline' 'unsafe-eval' https:",
        "StyleSrc": "'self' 'unsafe-inline' https:",
        "ImgSrc": "'self' data: https:",
        "FontSrc": "'self' data: https:",
        "ConnectSrc": "'self' wss: ws: http: https:",
        "FrameAncestors": "'none'",
        "BaseUri": "'self'",
        "FormAction": "'self'",
        "ObjectSrc": "'none'",
        "MediaSrc": "'self'",
        "ManifestSrc": "'self'",
        "WorkerSrc": "'self'",
        "ChildSrc": "'self'",
        "FrameSrc": "'none'",
        "UpgradeInsecureRequests": false
      }
    }
  }
}
